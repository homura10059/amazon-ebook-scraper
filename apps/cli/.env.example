# Amazon Ebook Scraper Configuration
# Copy this file to .env and fill in your actual values

# Discord Webhook URL (required)
# Get this from your Discord server settings -> Integrations -> Webhooks
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Scraper Settings (optional - defaults will be used if not set)
# Request timeout in milliseconds
SCRAPER_TIMEOUT=10000

# Number of retry attempts for failed requests
SCRAPER_RETRIES=3

# Delay between requests in milliseconds (to avoid rate limiting)
SCRAPER_DELAY=1000